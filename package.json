{
	"name": "pota.jsx",
	"type": "module",
	"scripts": {
		"dev": "node src/index.js"
	},
	"dependencies": {
		"@types/react": "^18.3.9",
		"oxc-parser": "^0.30.1",
		"preact": "^10.24.1",
		"prettier": "^3.3.3",
		"solid-js": "^1.9.1",
		"typescript": "^5.6.2",
		"voby": "^0.58.1",
		"vue": "^3.5.8"
